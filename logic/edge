- edge a b # a   f--k--j
- edge a c # | \    | 
- edge b c # b--c   g  h
- edge b d # |  |      |
- edge c e # d--e   i--l
- edge d e
- edge f k
- edge g k
- edge k j
- edge h l
- edge i l

- start a
WHEN reachable First {
  WHEN edge $First Second {
    - reachable $Second
  }
  WHEN edge Second $First {
    - reachable $Second
  }
}
WHEN edge One Two { - edge $Two $One }
WHEN start X { - reachable $X }

show reachable
